{
  "author": "TapGlue Team",
  "name": "tapglue-dashboard",
  "description": "Management Dashboard for users of tapglue.com .",
  "homepage": "https://dashboard.tapglue.com",
  "license": "ISC",
  "main": "index.js",
  "version": "0.1.0",
  "bugs": {
    "url": "https://github.com/tapglue/multiverse/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapglue/multiverse.git"
  },
  "dependencies": {
    "bootstrap": "~3.3.5",
    "clipboard": "~1.5.5",
    "d3": "~3.5.12",
    "fixed-data-table": "~0.6.0",
    "flux": "~2.1.1",
    "font-awesome": "~4.4.0",
    "humps": "~1.0.0",
    "js-cookie": "~2.0.3",
    "keymirror": "~0.1.1",
    "normalize.css": "~3.0.3",
    "react": "~0.14.5",
    "react-date-range": "^0.2.2",
    "react-dom": "^0.14.5",
    "react-dropdown": "^0.7.2",
    "react-faux-dom": "~2.1.1",
    "react-router": "~0.13.3",
    "reflex-grid": "=1.0.6",
    "whatwg-fetch": "~0.10.1"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-jest": "~6.0.1",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.3.13",
    "browser-sync": "^2.8.1",
    "css-loader": "^0.15.6",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.2.0",
    "eslint-plugin-react": "~3.13.1",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-eslint": "~1.1.1",
    "gulp-load-plugins": "~0.10.0",
    "gulp-util": "~3.0.6",
    "html-webpack-plugin": "^1.6.1",
    "jest": "~0.1.40",
    "jest-cli": "~0.8.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "node-static": "^0.7.7",
    "postcss-loader": "^0.5.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5"
  },
  "scripts": {
    "bundle": "gulp build:prod",
    "clean": "rm -rf build",
    "lint": "gulp lint",
    "serve": "gulp serve",
    "test": "jest",
    "watch": "gulp sync"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "setupEnvScriptFile": "<rootDir>/src/scripts/helpers/test-setup.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathDirs": [
      "src"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2",
    "unmockedModulePathPatterns": [
      "keyMirror",
      "react"
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "binaryLiterals": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": true,
      "generators": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": true,
      "regexUFlag": true,
      "regexYFlag": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true,
      "jsx": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "jasmine": true,
      "node": true
    },
    "globals": {
      "jest": true,
      "twttr": true
    },
    "rules": {
      "constructor-super": 2,
      "eol-last": 2,
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "max-len": [
        2,
        80,
        4
      ],
      "new-parens": 0,
      "no-underscore-dangle": [
        0
      ],
      "no-var": 2,
      "quotes": [
        2,
        "single"
      ],
      "react/display-name": 0,
      "react/jsx-boolean-value": 2,
      "react/jsx-no-undef": 2,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/no-did-mount-set-state": 2,
      "react/no-did-update-set-state": 2,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 2,
      "react/prop-types": 2,
      "react/react-in-jsx-scope": 2,
      "react/self-closing-comp": 2,
      "react/wrap-multilines": 2,
      "semi": [
        2,
        "never"
      ],
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "strict": [
        2,
        "global"
      ]
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ]
  }
}
